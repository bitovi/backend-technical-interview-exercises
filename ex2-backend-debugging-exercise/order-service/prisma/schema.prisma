generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model order {
  order_id Int   @id @default(autoincrement())
  customer Int?
  products Int[]
}

model product {
  product_id   Int     @id @default(autoincrement())
  product_name String? @db.VarChar(45)
  price        Int?
}
